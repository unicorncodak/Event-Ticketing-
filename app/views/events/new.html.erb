<div id="layoutSidenav_content">
    <main>
        
        <div class="container-fluid px-4">
        <h1 class="mt-4">New Event</h1>
        <% if notice %>
            <div class="alert alert-danger" role="alert">
                <%= notice %>
            </div>
        <% end %>
        <ol class="breadcrumb mb-4">
            <li class="breadcrumb-item"><a href="<%= events_path %>">Events</a></li>
            <li class="breadcrumb-item active">New Events</li>
        </ol>
        <div class="card mb-4">
            <div class="card-body">
                Events is a third party plugin that is used to generate the demo table below
            </div>
        </div>
        <%= render 'form', event: @event %>

        <%= link_to 'Back', events_path %>

        </div>
    </main>
</div>